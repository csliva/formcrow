<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/head %>
  </head>
  <body class="app">
    <header>
      <% include partials/header %>
    </header>

    <main>

      <a href="/dashboard/create">Create a new form</a>
      
      <ul>
        <% queries.forEach(function(entry) { %>
          <li class="color">
            <%= entry.query %>
            <a href="/dashboard/<%= entry._id %>">View</a>
          </li>
        <% }); %>
      </ul>
    </main>

    <footer>
      <% include partials/footer %>
    </footer>

  </body>
</html>
